<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewmodel.SignUpController">
    <TextField fx:id="userNameField" promptText="Enter your username" />
    <TextField fx:id="passwordField" promptText="Enter your password" />
    <TextField fx:id="confirmPasswordField" promptText="Confirm your password" />
    <Button text="Create Account" onAction="#createNewAccount"/>
    <Button text="Back to Login" onAction="#goBack"/>
</VBox>
